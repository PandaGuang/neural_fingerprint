{
  "data": {
    "dataset": "CIFAR10",
    "data_root": "data/cifar10",
    "batch_size": 128,
    "num_workers": 4,
    "transforms": {
      "resize": 32,
      "center_crop": 32,
      "normalize": {
        "mean": [0.4914, 0.4822, 0.4465],
        "std": [0.2023, 0.1994, 0.2010]
      }
    }
  },
  "models": {
    "architectures": ["vgg16", "resnet18", "densenet121"],
    "independent_model_count": 5,
    "pretrained": false
  },
  "fingerprint": {
    "num_samples": 100,
    "save_dir": "fingerprints",
    "target_class": 3,
    "generation_method": "random_noise"
  },
  "attacks": {
    "finetune": {
      "epochs": 10,
      "learning_rate": 0.0001,
      "performance_constraint": 0.80
    },
    "pruning": {
      "pruning_amount": 0.2,
      "performance_constraint": 0.80
    },
    "pruning_finetune": {
      "pruning_amount": 0.2,
      "finetune_epochs": 5,
      "learning_rate": 0.0001,
      "performance_constraint": 0.80
    },
    "model_extraction": {
      "epochs": 10,
      "learning_rate": 0.0001,
      "performance_constraint": 0.80
    },
    "adversarial_training": {
      "epochs": 10,
      "learning_rate": 0.0001,
      "performance_constraint": 0.80,
      "epsilon": 0.03
    }
  },
  "verification": {
    "threshold": 80.0
  },
  "paths": {
    "attacked_models_dir": "models/attacked",
    "fingerprint_file": "fingerprints/fingerprint.pt"
  }
}
