{
  "data": {
    "dataset": "ImageNet",
    "data_root": "data/imagenet",
    "batch_size": 32,
    "num_workers": 4,
    "transforms": {
      "resize": 256,
      "center_crop": 224,
      "normalize": {
        "mean": [0.485, 0.456, 0.406],
        "std": [0.229, 0.224, 0.225]
      }
    }
  },
  "models": {
    "architectures": ["vgg16", "resnet50", "densenet121"],
    "independent_model_count": 5,
    "pretrained": true
  },
  "fingerprint": {
    "num_samples": 100,
    "save_dir": "fingerprints",
    "target_class": 123,
    "generation_method": "random_noise"
  },
  "attacks": {
    "finetune": {
      "epochs": 5,
      "learning_rate": 0.0001,
      "performance_constraint": 0.95
    },
    "pruning": {
      "pruning_amount": 0.2,
      "performance_constraint": 0.95
    },
    "pruning_finetune": {
      "pruning_amount": 0.2,
      "finetune_epochs": 3,
      "learning_rate": 0.0001,
      "performance_constraint": 0.95
    },
    "model_extraction": {
      "epochs": 5,
      "learning_rate": 0.0001,
      "performance_constraint": 0.95
    },
    "adversarial_training": {
      "epochs": 5,
      "learning_rate": 0.0001,
      "performance_constraint": 0.95,
      "epsilon": 0.03
    }
  },
  "verification": {
    "threshold": 80.0
  },
  "paths": {
    "attacked_models_dir": "models/attacked",
    "fingerprint_file": "fingerprints/fingerprint.pt"
  }
}
